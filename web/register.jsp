    <!-- Login Form -->
    <form>
      <input type="text" id="name" class="fadeIn second" name="name" placeholder="Name">
      <input type="text" id="email" class="fadeIn third" name="email" placeholder="E-mail">
      <input type="text" id="contactNumber" class="fadeIn second" name="contactNumber" placeholder="Contact Number">
      <input type="text" id="password" class="fadeIn third" name="password" placeholder="Password">
      <input type="text" id="passwordAgain" class="fadeIn second" name="passwordAgain" placeholder="Retype Password">
      <input type="submit" class="fadeIn fourth" value="Sign Up">
    </form>

